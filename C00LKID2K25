-- FIX LAG + ·∫®N NG∆Ø·ªúI CH∆†I + XO√Å SKIN + XO√Å VI·ªÄN + XO√Å B·∫¶U TR·ªúI by trn.minh.tr0089
local Lighting = game:GetService("Lighting")
local Terrain = workspace:FindFirstChildOfClass("Terrain")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local StarterGui = game:GetService("StarterGui")

-- T·∫Øt hi·ªáu ·ª©ng √°nh s√°ng v√† s∆∞∆°ng m√π
Lighting.GlobalShadows = false
Lighting.FogEnd = 1e10
Lighting.Brightness = 0

-- Xo√° b·∫ßu tr·ªùi n·∫øu c√≥
pcall(function()
    local sky = Lighting:FindFirstChildOfClass("Sky")
    if sky then
        sky:Destroy()
    end
end)

-- Gi·∫£m ch·∫•t l∆∞·ª£ng ƒë·ªãa h√¨nh
if Terrain then
    Terrain.WaterWaveSize = 0
    Terrain.WaterWaveSpeed = 0
    Terrain.WaterReflectance = 0
    Terrain.WaterTransparency = 1
end

-- X√≥a c√°c hi·ªáu ·ª©ng √°nh s√°ng d∆∞ th·ª´a
pcall(function()
    for _, effect in pairs(Lighting:GetChildren()) do
        if effect:IsA("BloomEffect") or effect:IsA("BlurEffect") or effect:IsA("SunRaysEffect") or effect:IsA("ColorCorrectionEffect") then
            effect:Destroy()
        end
    end
end)

-- T·∫Øt Particle, Decals, Texture
for _, v in pairs(workspace:GetDescendants()) do
    if v:IsA("ParticleEmitter") or v:IsA("Trail") then
        v:Destroy()
    elseif v:IsA("Decal") or v:IsA("Texture") then
        v.Transparency = 1
    end
end

-- ·∫®n t·∫•t c·∫£ ng∆∞·ªùi ch∆°i kh√°c
for _, player in pairs(Players:GetPlayers()) do
    if player ~= LocalPlayer and player.Character then
        player.Character:Destroy()
    end
end

-- X√≥a skin nh√¢n v·∫≠t v√† x√≥a vi·ªÅn
if LocalPlayer.Character then
    for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
        if part:IsA("Accessory") or part:IsA("Clothing") or part:IsA("ShirtGraphic") or part:IsA("Pants") or part:IsA("Shirt") then
            part:Destroy()
        elseif part:IsA("BasePart") then
            part.Material = Enum.Material.Plastic
            part.Reflectance = 0
            part.Color = Color3.fromRGB(255, 255, 255)
            part.CastShadow = false
        elseif part:IsA("SurfaceAppearance") then
            part:Destroy()
        end
    end
end

StarterGui:SetCore("SendNotification", {
    Title = "script make by c00lkid2k25",
    Text = "script Vi·ªát Nam üáªüá≥!",
    Duration = 6
})
